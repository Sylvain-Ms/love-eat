<div class="profile_match">
  <%= image_tag "profile_picture.svg", id: "picture" %>
  <div class="profile_match_container">
    <h2><strong><%= match.user_liked.first_name %>, <%= match.user_liked.age %>ans</strong></h2>
    <p class="details"></p>
    <p><strong>Description:</strong> <%= match.user_liked.description %></p>
    <div class='tag_list'>
      <% match.user_liked.foodmoods.each do |foodmood| %>
        <div>
          <p><%= foodmood.name %></p>
        </div>
      <% end %>
    </div>
  </div>
</div>
